{
  "name": "$projectName",
  "version": "1.0.0",
  "author": "$author",
  "license": "UNLICENSED",
  "scripts": {
    "analyze": "node codegen/components && webpack --mode=production --env.analyzer",
    "analyze:static": "node codegen/pages && webpack --progress --mode=production --env.static --env.analyzer",
    "build": "node codegen/components && webpack --mode=production",
    "build:static": "node codegen/pages && webpack --mode=production --env.static",
    "dev": "node codegen/pages && webpack-dev-server --mode=development --env.static",
    "lint": "eslint --ext js,jsx ./source",
    "lint:fix": "eslint --ext js,jsx ./source --fix",
    "test:ui": "testcafe chrome:headless ./tests/*.js"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@creuna/codegen": "^0.2.0",
    "@creuna/utils": "^1.1.0",
    "autoprefixer": "^9.1.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^2.0.0",
    "cssnano": "^4.1.7",
    "eslint": "^5.5.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.0.1",
    "expose-loader": "^0.7.3",
    "file-loader": "^3.0.1",
    "import-glob": "^1.5.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.5.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.3",
    "prop-types": "^15.5.7",
    "pubsub-js": "^1.6.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^7.0.1",
    "static-site-generator-webpack-plugin": "^3.4.1",
    "suppress-chunks-webpack-plugin": "^1.0.5",
    "testcafe": "^0.23.2",
    "webpack": "^4.10.1",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.0.2",
    "webpack-dev-server": "^3.1.4",
    "webpack-manifest-plugin": "^2.0.3",
    "whatwg-fetch": "^3.0.0"
  }
}
