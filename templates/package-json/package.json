{
  "name": "$projectName",
  "version": "1.0.0",
  "author": "$author",
  "license": "UNLICENSED",
  "scripts": {
    "analyze": "node scripts/create-app-components && webpack --mode=production --env.analyzer",
    "analyze:static": "node scripts/create-app-components && webpack --progress --mode=production --env.static --env.analyzer",
    "build": "node scripts/create-app-components && webpack --mode=production",
    "build:static": "node scripts/create-app-components && node scripts/create-mockup-pages && webpack --mode=production --env.static",
    "component": "node scripts/create-component",
    "dev": "node scripts/create-app-components && node scripts/create-mockup-pages && webpack-dev-server --mode=development --env.static",
    "lint": "eslint --ext js,jsx ./source",
    "lint:fix": "eslint --ext js,jsx ./source --fix",
    "mockup": "node scripts/create-mockup-page",
    "rename": "node scripts/rename-component",
    "test:ui": "chimp ./chimpconfig.js",
    "to-stateful": "node scripts/convert-to-stateful-component",
    "to-stateless": "node scripts/convert-to-stateless-component"
  },
  "dependencies": {
    "@creuna/utils": "^0.3.3",
    "autoprefixer": "^8.5.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chalk": "^2.1.0",
    "chimp": "^0.51.1",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.8",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^2.2.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.0.1",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.6",
    "import-glob": "^1.5.0",
    "json-loader": "^0.5.4",
    "klaw": "^2.1.0",
    "lodash": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.5",
    "node-sass": "^4.5.1",
    "null-loader": "^0.1.1",
    "postcss-loader": "^2.0.10",
    "prettier": "^1.9.2",
    "prop-types": "^15.5.7",
    "pubsub-js": "^1.6.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^7.0.1",
    "static-site-generator-webpack-plugin": "^3.4.1",
    "suppress-chunks-webpack-plugin": "^0.0.4",
    "webpack": "^4.10.1",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-cli": "^3.0.2",
    "webpack-dev-server": "^3.1.4",
    "webpack-manifest-plugin": "^2.0.3",
    "whatwg-fetch": "^2.0.4"
  }
}
