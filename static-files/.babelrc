{
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread"
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "debug": false,
        "targets": {
          "browsers": ["last 2 versions"]
        },
        "useBuiltIns": "usage"
      }
    ],
    "@babel/preset-react"
  ]
}
